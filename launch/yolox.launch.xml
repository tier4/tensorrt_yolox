<launch>
  <arg name="image_raw0" default="/sensing/camera/camera0/image_rect_color"/>
  <arg name="image_raw1" default="/sensing/camera/camera1/image_rect_color"/>
  <arg name="image_raw2" default="/sensing/camera/camera2/image_rect_color"/>
  <arg name="image_raw3" default="/sensing/camera/camera3/image_rect_color"/>
  <arg name="image_raw4" default="/sensing/camera/camera4/image_rect_color"/>
  <arg name="image_raw5" default="/sensing/camera/camera5/image_rect_color"/>
  <arg name="image_number" default="6"/>

  <include if="$(eval &quot;'$(var image_number)'>='1'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw0)"/>
    <arg name="output_topic" value="rois0"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='2'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw1)"/>
    <arg name="output_topic" value="rois1"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='3'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw2)"/>
    <arg name="output_topic" value="rois2"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='4'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw3)"/>
    <arg name="output_topic" value="rois3"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='5'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw4)"/>
    <arg name="output_topic" value="rois4"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='6'&quot;)" file="$(find-pkg-share tensorrt_yolox)/launch/tensorrt_yolox.launch.xml">
    <arg name="input_topic" value="$(var image_raw5)"/>
    <arg name="output_topic" value="rois5"/>
  </include>
</launch>
